{
    "manifest_version":2,
    "name":"Page Override",
    "version":"1.0",
   
     "chrome_url_overrides":{
        "newtab":"pages/index.html"
     },
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["content.js"]
        }   
     ],
      
     "browser_action":{
         "default_icon":"images/logo.png"
     }
}
